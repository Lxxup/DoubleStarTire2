<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"

android:background="#ffffff"


    >

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"

        android:orientation="vertical">

        <RelativeLayout
            android:id="@+id/newhome_ll"
            android:layout_width="fill_parent"
            android:layout_height="35dp"
            android:background="#f4f5f6">

            <TextView
                android:id="@+id/tv_main_set"
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:layout_alignParentLeft="true"
                android:layout_centerVertical="true"
                android:layout_marginLeft="25dp"
                android:background="@drawable/main_set" />

            <!--<ImageView
                android:layout_width="160dp"
                android:layout_height="35dp"
                android:layout_centerHorizontal="true"
                android:layout_centerVertical="true"
                android:src="@drawable/new_icon" />-->

            <TextView
                android:id="@+id/tv_main_menu"
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="25dp"
                android:background="@drawable/menu_login"
                android:gravity="center" />

        </RelativeLayout>


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <ImageView
                android:layout_width="match_parent"
                android:layout_height="220dp"
                android:src="@drawable/firstpage_item"
                />



        </LinearLayout>


        <TextView
            android:text="已完成订单信息一"
            android:id="@+id/tv1_complete"
            android:textSize="20dp"
            android:layout_marginTop="20dp"
            android:textColor="#000000"
            android:gravity="center"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" />

        <TextView
            android:text="已完成订单信息二"
            android:layout_marginTop="40dp"
            android:id="@+id/tv2_complete"
            android:textSize="20dp"
            android:textColor="#000000"
            android:gravity="center"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" />

        <TextView
            android:text="已完成订单信息三"
            android:layout_marginTop="40dp"
            android:id="@+id/tv3_complete"
            android:textSize="20dp"
            android:textColor="#000000"
            android:gravity="center"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" />v

    </LinearLayout>

    <RelativeLayout
        android:padding="10dp"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="25dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <Button
            android:text="返回"
            android:id="@+id/btn_complete"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" />



    </RelativeLayout>



</RelativeLayout>
